$ cat rkurth.dat
$proto = 'https';
$proto = 'http';
$proto = 'ftp';
$proto = 'smtp';
$proto = 'nntp';
$ cat rkurth.sh
#!/bin/sh

# ORGINAL=/tmp/TESTSCRIPT/httpd.conf
ORGINAL=rkurth.dat
KOPY=/tmp/TESTSCRIPT/httpd.conf.back3
OUTPUT=/tmp/TESTSCRIPT/httpd.conf.new

# cp $ORGINAL $KOPY

sed -e "/\$proto = 'https';/a\
\$portnumber = '5001';
" < $ORGINAL # > $OUTPUT

# mv $OUTPUT $ORGINAL

exit 0

$ ./rkurth.sh
$proto = 'https';
$portnumber = '5001';
$proto = 'http';
$proto = 'ftp';
$proto = 'smtp';
$proto = 'nntp';
$

